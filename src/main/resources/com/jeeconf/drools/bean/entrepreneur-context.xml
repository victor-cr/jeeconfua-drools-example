<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="victor.polischuk" class="com.jeeconf.drools.bean.Entrepreneur">
        <constructor-arg index="0" value="Victor Polischuk"/>
        <constructor-arg index="1">
            <list value-type="com.jeeconf.drools.bean.Role">
                <value>IT</value>
            </list>
        </constructor-arg>
        <constructor-arg index="2" value="III"/>
    </bean>

    <bean id="moses.wasserman" class="com.jeeconf.drools.bean.Entrepreneur">
        <constructor-arg index="0" value="Moses Wasserman"/>
        <constructor-arg index="1" value="II"/>
    </bean>

    <bean id="julia.semechkina" class="com.jeeconf.drools.bean.Entrepreneur">
        <constructor-arg index="0" value="Julia Semechkina"/>
        <constructor-arg index="1" value="I"/>
    </bean>

    <bean id="ilia.muromets" class="com.jeeconf.drools.bean.Entrepreneur">
        <constructor-arg index="0" value="Ilia Muromets"/>
        <constructor-arg index="1">
            <list value-type="com.jeeconf.drools.bean.Role">
                <value>DISABLED</value>
            </list>
        </constructor-arg>
        <constructor-arg index="2" value="II"/>
    </bean>
</beans>